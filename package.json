{
    "name": "portfolio-builder",
    "version": "0.0.0",
    "license": "MIT",
    "private": true,
    "scripts": {
        "apps:dev": "nx run-many --target=serve --projects=web,server",
        "web:dev": "nx serve web",
        "server:dev": "nx serve server",
        "server:seed": "node dist/apps/server/cli.js seed",
        "build": "npm run clean && nx run-many --target=lint,build --configuration=production --projects=web,server",
        "build:web": "npm run clean && nx run-many --target=lint,test,build --configuration=production --projects=web",
        "test": "nx run-many --target=test --verbose",
        "clean": "rimraf dist",
        "lint": "nx run-many --target=lint",
        "docker": "npm run build && docker-compose up --force-recreate --build -d && docker image prune -f",
        "migration:create": "typeorm migration:create ./apps/server/src/app/migrations/${MIGRATION_NAME}",
        "migration:revert": "npx typeorm migration:revert -d ./apps/server/src/data-source.ts",
        "migration:run": "nx run server:migration:run",
        "migration:generate": "nx run server:migration:generate",
        "migration:build": "nx run server:buildMigrations"
    },
